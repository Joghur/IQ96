{
  "name": "iq96",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "tsc": "./node_modules/typescript/bin/tsc",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build": "cd android && ./gradlew bundleRelease && npm run openLinuxMintWindow",
    "openLinuxMintWindow": "nemo ./app/build/outputs/bundle/release",
    "start:reset": "npm start --reset-cache",
    "clear-cache": "rm -rf node_modules && npm install && rm -rf /tmp/metro-* && cd android && ./gradlew clean",
    "start": "react-native start",
    "lint": "npm tsc && eslint . --ext .js,.ts,.tsx --cache",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/progress-bar-android": "^1.0.4",
    "@react-native-community/progress-view": "^1.3.1",
    "@react-native-firebase/app": "^12.8.0",
    "@react-navigation/bottom-tabs": "^6.0.4",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.2.5",
    "firebase": "^9.1.1",
    "i18next": "^20.4.0",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-i18next": "^11.11.4",
    "react-native": "~0.64.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-pdf": "^6.3.0",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-vector-icons": "^8.1.0",
    "rn-fetch-blob": "^0.12.0",
    "short-uuid": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.5",
    "@types/react-native-vector-icons": "^6.4.9",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react-native-a11y": "^2.0.4",
    "eslint-plugin-testing-library": "^4.10.1",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-dom": "^17.0.2",
    "react-test-renderer": "17.0.1",
    "ts-jest": "^27.0.4",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
